@{
    ViewData["Title"] = "Home Page";
}
@{
    Layout=null;
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TurnosLaM</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TurnosLaM.styles.css" asp-append-version="true" />
    <link rel="shortcut icon" href="data:image/svg+xml;base64," />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&family=Play&family=Tajawal:wght@200;300;400;500;700;800;900&display=swap" rel="stylesheet">
</head> 
<div class="container">
    <div class="row">
        <div class="m-5 d-flex justify-content-center ">
            <h1>Ingresa por favor tu numero de documento</h1>
        </div>
        <div class="d-flex justify-content-center " >
            <div class="col-8 ms-5">
                <input type="text" class="form-control  border-primary border-2" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite su documento..." >
            </div>
        </div>
        <div class="d-flex justify-content-center " >
            <div class="card  border-secondary mt-5 card-tecla"  style="width: 500px; height: 400px; ">
                <div class="card-body m-4" style="width: 20rem; ">
                    <div  style="width: 40rem;">
                        <button class="bton-tecla">1</button>
                        <button class="bton-tecla">2</button>
                        <button class="bton-tecla">3</button>
                    </div>
                    <div  style="width: 40rem;">
                        <button class="bton-tecla">4</button>
                        <button class="bton-tecla">5</button>
                        <button class="bton-tecla">6</button>
                    </div>
                    <div  style="width: 40rem;">
                        <button class="bton-tecla">7</button>
                        <button class="bton-tecla">8</button>
                        <button class="bton-tecla">9</button>
                    </div>
                    
                    <div  style="width: 40rem;">
                        <button class="btn-0">0</button>
                        <button class="bton-tecla">Borrar</button>

                    </div>
                </div>
            </div>

        </div>
            <div class="d-flex justify-content-end">
                <button class="btn btn-success btn-continuar" onclick="hola()">Continuar  
                </button>
            </div>



    </div>
</div>